datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  status    String   @default("new") // Default value for status
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}


model Respondent {
  id       Int      @id @default(autoincrement())
  email    String   @unique
  createdAt DateTime @default(now())
}